[core]
	autocrlf = false
	safecrlf = true
	excludesfile = /Users/gavrilovp/.gitignore
[user]
	email = pavelgavrilov19@gmail.com
	name = Gavrilov Pavel
[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short --all
	type = cat-file -t
	dump = cat-file -p
[push]
	default = matching
